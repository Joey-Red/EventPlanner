{
  "name": "EventPlanner",
  "version": "0.0.0",
  "private": true,
  "description": "REST API back end, React front end full stack MERN app monorepo event planning web app.",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/Joey-Red/EventPlanner"
  },
  "keywords": [
    "static",
    "MERN",
    "Mongoose",
    "MongoDB",
    "Express",
    "React",
    "Node",
    "SASS",
    "Passport",
    "JSONWEBTOKENS",
    "JWT",
    "Axios",
    "bcrypt",
    "Router"
  ],
  "author": "Joey Dalrymple",
  "license": "ISC",
  "scripts": {
    "postinstall": "if [ $CLIENT_ENV ]; then npm run postinstall-client; elif [ $SERVER_ENV ]; then npm run postinstall-server; else echo no environment detected, please set CLIENT_ENV or SERVER_ENV; fi",
    "postinstall-client": "cd frontend && npm install && npm run build",
    "postinstall-server": "cd backend && npm install",
  "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^4.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "gapi-script": "^1.2.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-google-login": "^5.2.2",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "uuidv4": "^6.2.13",
    "web-vitals": "^2.1.4"
  }
}
